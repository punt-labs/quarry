{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "TextractOCR",
      "Effect": "Allow",
      "Action": [
        "textract:DetectDocumentText",
        "textract:StartDocumentTextDetection",
        "textract:GetDocumentTextDetection"
      ],
      "Resource": "*"
    },
    {
      "Sid": "S3ForTextract",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": "arn:aws:s3:::ocr-7f3a1b2e4c5d4e8f9a1b3c5d7e9f2a4b/*"
    },
    {
      "Sid": "SageMakerEmbedding",
      "Effect": "Allow",
      "Action": [
        "sagemaker:DescribeEndpoint",
        "sagemaker:InvokeEndpoint"
      ],
      "Resource": "arn:aws:sagemaker:us-west-1:975377310343:endpoint/quarry-embedding"
    }
  ]
}
